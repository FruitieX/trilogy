{
  "name": "trilogy",
  "version": "0.9.2",
  "description": "100% pure JS SQLite, built on Knex & SQL.js",
  "main": "dist/trilogy.js",
  "jsnext:main": "dist/trilogy.mjs",
  "scripts": {
    "prebuild": "eslint src tests",
    "build": "rimraf dist/** && rollup -c && npm run docs",
    "watch": "rimraf dist/** && rollup -c -w",
    "pretest": "npm run build",
    "test": "ava",
    "flow": "flow check",
    "docs": "rimraf docs/** && jsdoc -c .jsdoc.json",
    "pages": "git subtree split --prefix docs -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages",
    "prepublish": "npm test",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/citycide/trilogy.git"
  },
  "keywords": [
    "es6",
    "es2015",
    "es2016",
    "modules",
    "rollup",
    "knex",
    "sqlite",
    "sql.js"
  ],
  "author": "citycide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/citycide/trilogy/issues"
  },
  "files": [
    "src",
    "dist",
    "flow"
  ],
  "homepage": "https://github.com/citycide/trilogy#readme",
  "dependencies": {
    "arify": "^0.7.0",
    "babel-runtime": "^6.11.6",
    "fs-jetpack": "^0.9.2",
    "is-plain-obj": "^1.1.0",
    "knex": "^0.11.10",
    "lodash.foreach": "^4.5.0",
    "native-or-lie": "^1.0.2",
    "sql.js": "^0.3.2"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.1.6",
    "docdash": "citycide/docdash",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.17.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "flow-bin": "^0.32.0",
    "jsdoc": "3.4.0",
    "jsdoc-babel": "^0.2.1",
    "jsdoc-strip-async-await": "^0.1.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-watch": "^2.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "verbose": true
  }
}
